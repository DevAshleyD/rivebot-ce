{
  "extensionsLocationCache": true,
  "logger": {
    "console": {
      "transport": "console",
      "level": "info"
    }
  },
  "modelConfig": {
    "INTENT_MODEL_NAME": "intent.model",
    "MODELS_DIR": "model_files",
    "TRAIN_DATA_NAME": "intent.txt",
    "DEFAULT_FALLBACK_INTENT_NAME": "fallback",
    "DEFAULT_WELCOME_INTENT_NAME": "init_conversation"
  },
  "crfConfig": {
    "c1": 1.0,
    "c2": 0.001,
    "max_iterations": 300,
    "feature_possible_transitions": 1
  },
  "db": {
    "connection": "mongodb-memory",
    "dbName": "botscripts",
    "imports": true,
    "exports": true,
    "dataSource": [
      {
        "name": "memory",
        "driver": "memory",
        "active": true
      },
      {
        "name": "mongodb-memory",
        "driver": "mongodb-memory-server",
        "port": "27071",
        "version": "4.0.3",
        "active": true
      }
    ]
  },
  "api": {
    "useBaseModel": false,
    "connection": "mongodb",
    "dataSource": [
      {
        "name": "memory",
        "driver": "memory",
        "active": true
      },
      {
        "name": "mongodb",
        "driver": "mongoose",
        "host": "localhost",
        "port": "27017",
        "username": "",
        "password": "",
        "database": "botscripts",
        "active": true,
        "rs": false
      }
    ]
  },
  "express": {
    "static": "./public",
    "httpPort": 3000,
    "poweredBy": "BotScriptAI Community Edition",
    "views": {
      "ext": "ejs",
      "engine": "ejs",
      "renderer": "renderFile",
      "path": "views"
    }
  },
  "appSettings": "app.json"
}
